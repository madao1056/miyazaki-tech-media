RUN_KEYSTATIC=false

# WordPress REST API settings
WORDPRESS_API_URL=https://your-wordpress-site.com/wp-json/wp/v2
WORDPRESS_USERNAME=your_username
WORDPRESS_PASSWORD=your_password

# Optional: WordPress Application Password (recommended over basic auth)
WORDPRESS_APP_PASSWORD=your_application_password

# Google Analytics 4 設定
GA4_PROPERTY_ID=your_ga4_property_id_here
GA4_CREDENTIALS={"type":"service_account","project_id":"your-project","private_key_id":"...","private_key":"-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n","client_email":"your-service-account@your-project.iam.gserviceaccount.com","client_id":"...","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"https://www.googleapis.com/robot/v1/metadata/x509/your-service-account%40your-project.iam.gserviceaccount.com"}

# 代替: サービスアカウントキーファイルのパス
# GA4_SERVICE_ACCOUNT_KEY_FILE=path/to/service-account-key.json

# 開発環境用（Analytics APIを無効にしてフォールバック使用）
# GA4_PROPERTY_ID=
# GA4_CREDENTIALS=